{
  "name": "fixture-simple-dynamic",
  "type": "module",
  "scripts": {
    "serve": "npx http-server -c=-1 -p=3000 .",
    "build": "rm -rf ./dist && npx parcel build --no-cache --no-content-hash",
    "profile": "mkdir -p profiles && NODE_ENV=production node --prof dist/index.js && node --prof-process isolate-0x*.log > profiles/processed.txt && rm isolate-0x*.log"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "parcel": "^2.11.0"
  },
  "targets": {
    "default" :{
      "source": "src/index.js",
      "distDir": "./parcel",
      "outputFormat": "esmodule",
      "optimize": true,
      "sourceMap": false
    }
  }
}
